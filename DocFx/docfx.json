{
  "metadata": [
      {
          "src": [
              {
                  "src": "../../GeometricalInvaders",
                  "files": [
                      "ElectricDrill.SimpleRPGCore.Runtime.csproj"
                  ],
                  "exclude": [
                      "Editor/**",
                      "bin/**"
                  ]
              }
          ],
          "globalNamespaceId": "Global",
          "filter": "filterConfig.yml",
          "dest": "api",
          "allowCompilationErrors": true,
          "properties": {
              "ProduceReferenceAssembly": "true"
          }
      }
  ],
  "build": {
      "content": [
          {
              "files": [
                  "**/*.{md,yml}"
              ],
              "exclude": [
                  "_site/**"
              ]
          },
          {
              "files": [
                  "api/toc.yml"
              ]
          }
      ],
      "resource": [
          {
              "files": [
                  "images/**"
              ]
          }
      ],
      "output": "../docs",
      "template": [
        "default",
        "modern"
      ],
      "globalMetadata": {
          "_appName": "Soap RPG Framework",
          "_appTitle": "Soap RPG Framework",
          "_appLogoPath": "images/soap-rpg-framework-logo.png",
          "_appFaviconPath": "images/favico.ico",
          "_enableSearch": true,
          "pdf": true
      },
      "xref": [
          "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
      ]
  }
}